        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="118dp"
        tools:listitem="@layout/category_fragment_item" />

</androidx.constraintlayout.widget.ConstraintLayout>
